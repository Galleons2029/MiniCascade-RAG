# ------------------------------
# Environment Variables for API service & worker
# ------------------------------

# ------------------------------
# Common Variables
# ------------------------------

# 硅基流动 API (必填 - 获取地址: https://siliconflow.cn/)
API_KEY=your_silicon_api_key_here

# LangGraph Agent 配置
LLM_API_KEY=your_silicon_api_key_here
LLM_MODEL=Qwen/Qwen2.5-7B-Instruct
DEFAULT_LLM_TEMPERATURE=0.2
MAX_TOKENS=2000

# 项目配置
PROJECT_NAME=MiniCascade-RAG
DEBUG=true
ENVIRONMENT=development

# 数据库配置
POSTGRES_URL=postgresql://admin:password@localhost:5433/postgres
DATABASE_URL=postgresql://admin:password@localhost:5433/postgres

# Qdrant 向量数据库
QDRANT_DATABASE_HOST=localhost
QDRANT_DATABASE_PORT=6333
USE_QDRANT_CLOUD=false

# Redis/Dragonfly 缓存
DRAGONFLY_HOST=localhost
DRAGONFLY_PORT=6381

# RabbitMQ 消息队列
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5673
RABBITMQ_DEFAULT_USERNAME=guest
RABBITMQ_DEFAULT_PASSWORD=guest
RABBITMQ_QUEUE_NAME=rag_test

# JWT 配置
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_DAYS=30

# Langfuse 可观测性 (可选)
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=https://cloud.langfuse.com

# LangSmith 追踪和调试 (可选)
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=MiniCascade-RAG
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com